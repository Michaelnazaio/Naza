programa {
  funcao inicio() {

    real numeroUm, numeroDois, mediaNumeros

    escreva("Digite um número: ")
    leia(numeroUm)

    escreva("Digite outro número: ")
    leia(numeroDois)

    mediaNumeros = (numeroUm + numeroDois) / 2

    escreva("A média desses dois números é: " + mediaNumeros)
    
    
  }
}
